{
  "name": "kanban-gaming-board-server",
  "version": "3.0.0",
  "description": "The server side application for kanban gaming board",
  "engines": {
    "node": "10.15.0"
  },
  "scripts": {
    "dev": "run-p dev:client dev:server",
    "dev:client": "cd react-ui/ && npm start",
    "dev:server": "set NODE_ENV=development && node server",
    "start": "set NODE_ENV=production && node server",
    "build": "cd react-ui/ && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "firebase": "^7.0.0",
    "firebase-admin": "^8.6.0",
    "jwks-rsa": "^1.6.0",
    "npm-run-all": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pvnetto/Kanban-Gaming-Board-React"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {}
}
